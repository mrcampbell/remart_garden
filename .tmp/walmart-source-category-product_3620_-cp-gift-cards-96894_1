<script>"use strict"; (function () { var menu = document.getElementsByClassName("vertical-tabber-control"); var tabContent = document.getElementsByClassName("tabber-content"); var content = document.getElementsByClassName("vertical-tabber"); var firstElementLabel = menu[0].getAttribute("aria-label"); menu[0].setAttribute("aria-label", firstElementLabel); menu[0].setAttribute("aria-selected", "true"); tabContent[0].setAttribute("aria-hidden", "false"); var cName = " active"; menu[0].className += cName; tabContent[0].className += cName; var accordionButtons = document.getElementsByClassName("accordionButton"); function onPressTabContentDisplay(e) { if (e.keyCode === 13 || e.keyCode === 32) { tabContentDisplay(e); } } function tabContentDisplay(e) { var target = e && e.currentTarget; var targetElement = target.id; var expanded = e.currentTarget.getAttribute("aria-selected"); var select = e.currentTarget.getAttribute("aria-label"); for (var i = 0; i < tabContent.length; i++) { menu[i].classList.remove("active"); tabContent[i].classList.remove("active"); menu[i].setAttribute("aria-selected", "false"); var removeSelect = menu[i].getAttribute("aria-label"); menu[i].setAttribute("aria-label", removeSelect); tabContent[i].setAttribute("aria-hidden", "true"); } e.currentTarget.setAttribute("aria-selected", "true"); e.currentTarget.setAttribute("aria-label", select); tabContent[targetElement].setAttribute("aria-hidden", "false"); target.className += cName; tabContent[target.id].className += cName; } function onPressChangeDropdown(e) { if (e.keyCode === 13 || e.keyCode === 32) { changeDropdown(e); } } function onPressChangeDropdown(e) { if (e.keyCode === 13 || e.keyCode === 32) { changeDropdown(e); } } function changeDropdown(e) { var expanded = e.currentTarget.getAttribute("aria-expanded"); var elementId = e.currentTarget.id; var parent = document.getElementById(elementId).parentElement; var liList = parent.querySelectorAll("li"); var aList = parent.querySelectorAll("a"); var textArea = parent.querySelector(".text"); var selectedContent = "#content-" + elementId.split("-")[2]; var ariaHiddenElement = parent.querySelector(selectedContent); var cName = "expanded"; var arr = parent.className.split(" "); if (expanded === "true") { e.currentTarget.setAttribute("aria-expanded", "false"); parent.className = "item"; ariaHiddenElement.setAttribute("aria-hidden", "true"); textArea.style.maxHeight = null; } else if (expanded === "false") { e.currentTarget.setAttribute("aria-expanded", "true"); if (arr.indexOf(cName) == -1) { parent.className += " " + cName; } ariaHiddenElement.setAttribute("aria-hidden", "false"); textArea.style.maxHeight = "100%"; } } function addListener(className, action, func) { for (var i = 0; i < className.length; i++) { className[i].addEventListener(action, func, false); } } addListener(menu, "click", tabContentDisplay); addListener(menu, "keypress", onPressTabContentDisplay); addListener(accordionButtons, "click", changeDropdown); addListener(accordionButtons, "keypress", onPressChangeDropdown); })();</script>

