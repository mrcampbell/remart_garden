ontent[0].setAttribute(\"aria-hidden\", \"false\"); var cName = \" active\"; menu[0].className += cName; tabContent[0].className += cName; var accordionButtons = document.getElementsByClassName(\"accordionButton\"); function onPressTabContentDisplay(e) { if (e.keyCode === 13 || e.keyCode === 32) { tabContentDisplay(e); } } function tabContentDisplay(e) { var target = e \u0026\u0026 e.currentTarget; var targetElement = target.id; var expanded = e.currentTarget.getAttribute(\"aria-selected\"); var select = e.currentTarget.getAttribute(\"aria-label\"); for (var i = 0; i \u003c tabContent.length; i++) { menu[i].classList.remove(\"active\"); tabContent[i].classList.remove(\"active\"); menu[i].setAttribute(\"aria-selected\", \"false\"); var removeSelect = menu[i].getAttribute(\"aria-label\"); menu[i].setAttribute(\"aria-label\", removeSelect); tabContent[i].setAttribute(\"aria-hidden\", \"true\"); } e.currentTarget.setAttribute(\"aria-selected\", \"true\"); e.currentTarget.setAttribute(\"aria-label\", select); tabContent[targetElement].setAttribute(\"aria-hidden\", \"false\"); target.className += cName; tabContent[target.id].className += cName; } function onPressChangeDropdown(e) { if (e.keyCode === 13 || e.keyCode === 32) { changeDropdown(e); } } function onPressChangeDropdown(e) { if (e.keyCode === 13 || e.keyCode === 32) { changeDropdown(e); } } function changeDropdown(e) { var expanded = e.currentTarget.getAttribute(\"aria-expanded\"); var elementId = e.currentTarget.id; var parent = document.getElementById(elementId).parentElement; var liList = parent.querySelectorAll(\"li\"); var aList = parent.querySelectorAll(\"a\"); var textArea = parent.querySelector(\".text\"); var selectedContent = \"#content-\" + elementId.split(\"-\")[2]; var ariaHiddenElement = parent.querySelector(selectedContent); var cName = \"expanded\"; var arr = parent.className.split(\" \"); if (expanded === \"true\") { e.currentTarget.setAttribute(\"aria-expanded\", \"false\"); parent.className = \"item\"; ariaHiddenElement.setAttribute(\"aria-hidden\", \"true\"); textArea.style.maxHeight = null; } else if (expanded === \"false\") { e.currentTarget.setAttribute(\"aria-expanded\", \"true\"); if (arr.indexOf(cName) == -1) { parent.className += \" \" + cName; } ariaHiddenElement.setAttribute(\"aria-hidden\", \"false\"); textArea.style.maxHeight = \"100%\"; } } function addListener(className, action, func) { for (var i = 0; i \u003c className.length; i++) { className[i].addEventListener(action, func, false); } } addListener(menu, \"click\", tabContentDisplay); addListener(menu, \"keypress\", onPressTabContentDisplay); addListener(accordionButtons, \"click\", changeDropdown); addListener(accordionButtons, \"keypress\", onPressChangeDropdown); })();\u003c/script\u003e\n\n\u003cdiv style=\"display:none\" class=\"moduleData\" data-codetype-check='CP' data-html='[{\"type\":\"MenuContent\",\"data\":{\"title\":\"Quick help\",\"verticalTabMenus\":[{\"menuName\":\"Using your gift card\",\"menuHeading\":\"Using Walmart gift cards\",\"menuContents\":[{\"type\":\"text\",\"contents\":\"\u003cp\u003e\u003cstrong\u003eHow to use your gift card to make online purchases:\u003c/strong\u003e\u003cbr\u003e\\nAt checkout, you can use up to five gift cards at one time or a combination of a gift card and other payment method such as credit/debit card or Pay with Cash. \u003c/p\u003e\\n\u003cp\u003e\u003cstrong\u003eTo use your gift cards online:\u003cbr\u003e\\n\u003c/strong\u003e1. During checkout you will reach the Payment Method screen.\u003cbr\u003e\\n2. Select Gift Card.\u003cbr\u003e\\n3. Add a new Gift Card or apply those already saved.\u003cbr\u003e\\n4. As cards are added, you will see them appear in your Purchase Summary.\u003cbr\u003e\\n5. If your order total is less than the amount on your Walmart Gift Card, the remaining amount will be available on your card for future purchases. If your order total is more than the amount on your Walmart Gift Card, you will need to pay for